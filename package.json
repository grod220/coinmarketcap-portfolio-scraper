{
  "name": "coin-mkt-cap-taxes",
  "version": "2.0.0",
  "description": "Capital gains calculator for CoinMarketCap portfolio exports",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node build/app",
    "typecheck": "tsc -p tsconfig.tests.json --noEmit",
    "lint": "npm run typecheck && biome lint src tests",
    "test": "npm run build && node --import tsx --test tests/*.test.ts",
    "format": "prettier --write \"{src,tests}/**/*.ts\" \"*.{json,md}\"",
    "format:check": "prettier --check \"{src,tests}/**/*.ts\" \"*.{json,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grod220/coinmarketcap-portfolio-scraper.git"
  },
  "author": "Gabe Rodriguez",
  "license": "ISC",
  "dependencies": {
    "date-fns": "^4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^25.2.3",
    "prettier": "^3",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "undici-types": "^7.21.0"
  },
  "overrides": {
    "undici-types": "^7.21.0"
  }
}
